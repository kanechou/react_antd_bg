<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>SSD微信公众号-产品信息</title>
    <% include  public/head%>
    <link rel="stylesheet" type="text/css" href="./../public/front/style/pic.css">
</head>

<body class="products_cat">
<div class="container-fluid">
    <div class="content">
        <h3> <a href="./data" class=" back">产品图片</a></h3>

        <div class="mgtop">
            <div class="row ">
                <% result.forEach(function(x){ %>
                <div class="col-sm-6 col-md-6 in1">
                    <div class="row">
                        <div class="col-xs-7 col-lg-7">
                            <a href="pics?categoryId=<%= x.Id%>"><img src="../<%= x.Pic%>" class="img-responsive"></a>
                        </div>
                        <div class="col-xs-5 col-lg-5">
                            <div class="product_text">
                                <h3><a href="pics?categoryId=<%= x.Id%>"><%= x.Name%></a></h3>
                                <div class="read"><a href="pics?categoryId=<%= x.Id%>">浏览图片</a></div>
                            </div>
                        </div>
                    </div>
                </div>
                <% })%>


            </div>
            <!--row-->
        </div>
    </div>
</div>
<!--container-fluid-->
</body>

</html>
